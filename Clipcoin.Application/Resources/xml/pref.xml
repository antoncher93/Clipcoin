<?xml version="1.0" encoding="utf-8" ?>
<PreferenceScreen
    xmlns:android="http://schemas.android.com/apk/res/android">
	<EditTextPreference
		android:value ="@string/tel_send_address"
        android:key="telemetry_address"
        android:summary="Address for sending telemetry"
        android:title="Address">
	</EditTextPreference>
	<ListPreference
	   android:entries="@array/entries"
	   android:entryValues="@array/entries"
	   android:key="beacon_scan_interval"
	   android:summary="Interval of Beacon Scanning"
	   android:title="Scan Interval">
	</ListPreference>
</PreferenceScreen>